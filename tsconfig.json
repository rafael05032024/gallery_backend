{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./src",
    "outDir": "./dist",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "paths": {
      "@/*": ["*"]
    },
  },
}
